<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>amir</name>
  <version>0.0.0</version>
  <description>Metapackage that contains all essential packages for AMIR 740</description>
  <author email="jie@vstone.co.jp">Jie</author>
  <maintainer email="infodesk@vstone.co.jp">Vstone</maintainer>
  <license>Apache License 2.0</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <exec_depend>amir_bringup</exec_depend>
  <exec_depend>amir_description</exec_depend>
  <exec_depend>amir_driver</exec_depend>
  <exec_depend>amir_interfaces</exec_depend>
  <exec_depend>amir_moveit_config</exec_depend>

  <test_depend>ament_lint_auto</test_depend>
  <test_depend>ament_lint_common</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
